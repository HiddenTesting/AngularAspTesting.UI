<form name="feedbackForm" class="form-horizontal">
    <div class="form-group">
        <span class="col-md-2">Name</span>
        <div class="col-md-6">
            <input type="text" required class="form-control" ng-model="feedbackModel.Name"/>
        </div>
    </div>
    <div class="form-group">
        <span class="col-md-2">Email</span>
        <div class="col-md-6">
            <input type="text" required class="form-control" name="emailAddress" ng-model="feedbackModel.EmailAddress"/>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-4">
            <button class="btn btn-primary" ng-click="SubmitEnterFeedbackData()" ng-disabled="feedbackForm.$invalid">Submit »</button>
        </div>
    </div>
</form>